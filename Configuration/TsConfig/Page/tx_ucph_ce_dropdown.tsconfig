####################################
#### NEW CONTENT ELEMENT WIZARD ####
####################################
mod.wizards.newContentElement.wizardItems.interactive {
    elements {
        ucph_content_dropdown {
            iconIdentifier = ucph-ce-dropdown-icon
            title = LLL:EXT:ucph_content_dropdown/Resources/Private/Language/locallang_be.xlf:title
